        <TextField fx:id="fieldBeg" layoutX="14.0" layoutY="19.0" prefHeight="25.0" prefWidth="162.0" promptText="Начало" />
        <TextField fx:id="fieldEnd" layoutX="191.0" layoutY="19.0" prefHeight="25.0" prefWidth="162.0" promptText="Конец" />
        <TextField fx:id="fieldStep" layoutX="367.0" layoutY="19.0" prefHeight="25.0" prefWidth="162.0" promptText="Шаг" />
        <Button layoutX="367.0" layoutY="57.0" mnemonicParsing="false" onMouseClicked="#calculate" prefHeight="25.0" prefWidth="162.0" text="Вычислить" />
        <TextArea fx:id="fieldTab" editable="false" layoutX="263.0" layoutY="100.0" prefHeight="289.0" prefWidth="266.0" promptText="Табуляция функции" />
        <Label fx:id="labelInfo" alignment="TOP_LEFT" layoutX="11.0" layoutY="100.0" prefHeight="145.0" prefWidth="252.0" text="Информация о табуляции:" />
    </children>
